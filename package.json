{
  "name": "gbox-sdk",
  "description": "A TypeScript/JavaScript client library for interacting with GBox sandbox services, enabling Android and Terminal environment operations.",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "gbox",
    "android",
    "automation",
    "sandbox",
    "terminal",
    "emulator"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/babelcloud/gbox-sdk-alpha.git"
  },
  "author": "BabelCloud",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/babelcloud/gbox-sdk-alpha/issues"
  },
  "homepage": "https://github.com/babelcloud/gbox-sdk-alpha#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "lodash": "^4.17.21",
    "open": "^10.1.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.17.48",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  }
}
